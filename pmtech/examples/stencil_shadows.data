    float lut_bias = 0.5/lut_size;
    float theta = acos(dot(n, v));
    float2 uv = float2(roughness, theta / (0.5 * pi));
    uv = uv * lut_scale + lut_bias;
    float4 mat = sample_texture(ltc_mat, uv);
