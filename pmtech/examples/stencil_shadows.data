        nh_min9 = min(nh_min9, c);
        nh_max9 = max(nh_max9, c);
        if(d < frame_depth)
        {
            tc2 = ctc + samples[i];
            frame_depth = d;
        }
    }
    float4 nh_min = lerp(nh_min9, nh_min5, 0.5);
    float4 nh_max = lerp(nh_max9, nh_max5, 0.5);
