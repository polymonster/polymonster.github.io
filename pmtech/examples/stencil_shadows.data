    float3 vl = normalize(world_pos.xyz - light_pos_radius.xyz);
    float3 sd = normalize(light_dir_cutoff.xyz);
    float dp = (1.0 - dot(vl, sd));
    return smoothstep(co, co - falloff, dp);
