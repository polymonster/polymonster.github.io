    float4 nh_max9 = float4(0.0, 0.0, 0.0, 0.0);
    float4 nh_min5 = float4(1.0, 1.0, 1.0, 1.0);
    float4 nh_max5 = float4(0.0, 0.0, 0.0, 0.0);
