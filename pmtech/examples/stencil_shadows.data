    bone_indices[2] = int(indices.z);
    bone_indices[3] = int(indices.w);
    float3 rt = float3( 0.0, 0.0, 0.0);
    float3 rb = float3( 0.0, 0.0, 0.0);
    float3 rn = float3( 0.0, 0.0, 0.0);
