        if(uvw.y >= 1.0 || uvw.y <= 0.0)
        discard;
        if(uvw.z >= 1.0 || uvw.z <= 0.0)
        discard;
        if( d <= 0.01 )
        break;
    }
    float vd = (1.0 - d);
