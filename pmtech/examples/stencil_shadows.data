    nor.x = map_torus(pos+eps.xyy) - map_torus(pos-eps.xyy);
    nor.y = map_torus(pos+eps.yxy) - map_torus(pos-eps.yxy);
    nor.z = map_torus(pos+eps.yyx) - map_torus(pos-eps.yyx);
    return normalize(nor);
}
