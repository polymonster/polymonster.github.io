            profile *= albedo.rgb;
            lit_colour.rgb += profile * sss;
            lit_colour += albedo.rgb * 0.15;
            ++shadow_map_index;
        }
        lit_colour += oren_nayar(
        lights[i].pos_radius * -1.0,
        lights[i].colour.rgb * 0.25,
        n,
        _input.world_pos.xyz,
        camera_view_pos.xyz,
        1.0 - roughness,
        albedo.rgb
        );
    }
